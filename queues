#include <stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *link;
};
struct node *front=NULL;
struct node *rear=NULL;
int isempty(){
    return (front==NULL);
}

void enqueue(int data ){
    struct node *newnode=malloc(sizeof(struct node));
    newnode->data=data;
    newnode->link=NULL;
    if(rear==NULL){
        rear=front=newnode;
    }
    else{
        rear->link=newnode;
        rear=newnode;
    }
}
int dequeue(){
    if(front==NULL){
        printf("the queue is empty");
        return -1;
    }
   
        struct node* temp=front;
        int data=temp->data;
        front=front->link;
        if(front==NULL){
            rear==NULL;
        }
        free(temp);
        return data;
   
}
void display(){
    if(front==NULL){
        printf("linked list is empty");
    }
    struct node *ptr=NULL;
    ptr=front;
    while(ptr!=NULL){
        printf("%d  ",ptr->data);
        ptr=ptr->link;
    }
}
int main(){
    enqueue(20);
    enqueue(30);
    enqueue(40);
    printf("%d\n",dequeue());
    display();
}

